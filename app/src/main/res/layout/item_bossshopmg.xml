<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="@dimen/dd_dimen_20px"
    android:paddingBottom="@dimen/dd_dimen_10px"
    android:paddingLeft="@dimen/dd_dimen_50px"
    android:orientation="vertical">


    <TextView
        android:id="@+id/tv_bossshopmg_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:textColor="@color/wordcolor"
        />


    <TextView
        android:id="@+id/tv_bossshopmg_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/viewline"
        android:textSize="17sp"/>

</LinearLayout>
